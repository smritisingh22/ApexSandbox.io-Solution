public Boolean isPrime(Integer num) {
    //code here

    Boolean primeFlag;
    if(num = 2 || num = 3){
        primeFlag=true;
    }else{
        for(Integer i=2;i<=(num/2);i++)
        {
            if(Math.mod(num,i)==0)
            {
                primeFlag=false;
                break;
            }
            else
            {
                primeFlag=true;
                break;
            }
        }
    }
    return primeFlag;
}