public Id insertStudent(String name, String email) {
    // code here

    /*apxio__Student__c student = new apxio__Student__c();
    student.Name=name;
    student.apxio__Email__c=email;
    insert student;
    return student.Id;*/

     // Create a new student record
        apxio__Student__c student = new apxio__Student__c();
        student.Name = name;
        student.apxio__Email__c = email;

        // Insert the record
        insert student;

        // Return the Id of the newly created record
        return student.Id;
}